<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="basement-finderPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>models.Basementfeatures</class>
    <class>models.Basementamenities</class>
    <class>models.Basements</class>
    <class>models.Users</class>
    <class>models.Amenities</class>
    <class>models.Basementimages</class>
    <class>models.Features</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <non-jta-data-source>java:/comp/env/jdbc/basementfinderdb</non-jta-data-source>
  </persistence-unit>
</persistence>
